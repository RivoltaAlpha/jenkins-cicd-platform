{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "title": "Jenkins Performance",
  "tags": ["jenkins", "ci-cd"],
  "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Jenkins Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=\"jenkins\"}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Total Builds",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "default_jenkins_builds_total_build_count_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 3,
        "title": "Build Success Rate",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "(default_jenkins_builds_success_build_count_total / default_jenkins_builds_total_build_count_total) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Executor Queue Length",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "jenkins_executor_queue_length",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 3, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Build Duration Over Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "rate(default_jenkins_builds_duration_milliseconds_summary_sum[5m]) / rate(default_jenkins_builds_duration_milliseconds_summary_count[5m])",
            "refId": "A",
            "legendFormat": "Avg Build Duration"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Duration"}
        ]
      },
      {
        "id": 6,
        "title": "Build Rate (builds/min)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "rate(default_jenkins_builds_total_build_count_total[5m]) * 60",
            "refId": "A",
            "legendFormat": "Total Builds"
          },
          {
            "expr": "rate(default_jenkins_builds_success_build_count_total[5m]) * 60",
            "refId": "B",
            "legendFormat": "Successful Builds"
          },
          {
            "expr": "rate(default_jenkins_builds_failed_build_count_total[5m]) * 60",
            "refId": "C",
            "legendFormat": "Failed Builds"
          }
        ]
      },
      {
        "id": 7,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "container_memory_usage_bytes{name=\"jenkins\"} / 1024 / 1024",
            "refId": "A",
            "legendFormat": "Memory Usage (MB)"
          }
        ],
        "yaxes": [
          {"format": "MB"}
        ]
      },
      {
        "id": 8,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{name=\"jenkins\"}[5m]) * 100",
            "refId": "A",
            "legendFormat": "CPU Usage %"
          }
        ],
        "yaxes": [
          {"format": "percent"}
        ]
      }
    ],
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "version": 1,
    "uid": "jenkins-perf-001",
    "schemaVersion": 36
}